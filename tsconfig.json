{
  "extends": "@react-native/typescript-config",
  "compilerOptions": {
    "module": "nodenext",
    "lib": [
      "es2017"
    ],
    "jsx": "react-jsx",
    "moduleResolution": "nodenext",
    "strict": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "noImplicitThis": true,
    "alwaysStrict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "forceConsistentCasingInFileNames": true
  },
}
