{
  "name": "myApp",
  "version": "0.4.0-beta.6",
  "private": true,
  "scripts": {
    "android": "react-native run-android && adb shell am start -n com.moujitx.myapp.dev/com.moujitx.myapp.MainActivity",
    "ios": "react-native run-ios",
    "lint": "eslint . --fix",
    "start": "react-native start",
    "test": "jest",
    "prettier": "prettier --list-different --write 'src/**/*.{ts,tsx,json}'",
    "pod": "cd ios && pod install",
    "buildClean": "rm -rf android/.gradle android/app/build android/app/.cxx",
    "buildKey": "keytool -genkey -v -keystore android/app/my-release-key.keystore -alias my-key-alias -keyalg RSA -keysize 2048 -validity 10000",
